<% if current_user%>
<h1>All Contacts</h1>
  <% @contacts.each do |contact|%>
    <% if current_user.id==contact.user_id%>
      <h2><a href="/contacts/<%=contact.id%>"><%=contact.name%></a></h2>
      <h3><%=contact.middle_name%></h3>
      <h3><%=contact.phone%></h3>
      <h3><%=contact.email%></h3>
      <h3><%=contact.birthday%></h3>
    <% end %>
  <% end %>
  <a href="/contacts/new">Add New Contact</a>
  <% else flash[:warning] = "Please Log In or Signup" %>
<% end %>

